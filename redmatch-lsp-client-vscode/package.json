{
	"name": "redmatch-lsp-client",
	"description": "VSCode part of the Redmatch language server",
	"author": "CSIRO",
	"license": "MIT",
	"version": "1.0.0",
	"publisher": "csiro",
	"categories": [],
	"engines": {
		"vscode": "^1.52.0"
	},
	"activationEvents": [
		"onLanguage:redmatch"
	],
	"main": "./out/extension",
	"contributes": {
		"languages": [{
			"id": "redmatch",
			"extensions": [ ".rdm" ],
			"aliases": [ "Redmatch", "redmatch" ]
		}]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -b",
		"watch": "tsc -b -w",
		"lint": "eslint ./src --ext .ts,.tsx"
	},
	"dependencies": {
		"vscode-languageclient": "^7.0.0",
		"find-java-home": "^1.2.2",
		"fs-extra": "^10.0.0",
		"lodash": "^4.17.21",
		"expand-home-dir": "^0.0.3",
		"winreg-utf8": "^0.1.1"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/mocha": "^8.2.2",
		"@types/node": "^12.12.0",
		"@types/winreg": "^1.2.31",
		"@vscode/test-electron": "^1.6.1",
		"@typescript-eslint/eslint-plugin": "^4.23.0",
		"@typescript-eslint/parser": "^4.23.0",
		"eslint": "^7.26.0",
		"mocha": "^8.3.2",
		"typescript": "^4.3.5"
	}
}
